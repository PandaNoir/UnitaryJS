{
  "name": "unitaryjs",
  "version": "0.1.1",
  "description": "Canvas Library",
  "main": "./dist/unitary.min.js",
  "scripts": {
    "preversion": "npm test",
    "version": "npm run build && gulp && git add -A",
    "postversion": "git push && git push --tags",
    "pretest": "npm run build:unitary",
    "test": "mocha -R spec",
    "build": "rollup src/main.js -c -o dist/unitary.js --name Unitary",
    "build:unitary": "rollup src/unitary.js -c -o dist/onlyunitary.js --name Unitary",
    "build2": "rollup src/canvas.js -c -o dist/canvas.js --name Canvas"
  },
  "files": [
    "CHANGELOG.md",
    "LICENCE",
    "README.md",
    "package.json",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pandanoir/unitaryjs.git"
  },
  "keywords": [
    "canvas",
    "library",
    "unitaryjs"
  ],
  "devDependencies": {
    "gulp": "3.9.1",
    "gulp-closure-compiler": "0.4.0",
    "gulp-gzip": "1.2.0",
    "gulp-rename": "1.2.2",
    "jest": "22.1.1",
    "mocha": "3.1.2",
    "rollup": "0.36.3",
    "rollup-plugin-json": "2.0.2"
  },
  "author": "Naoto Ikuno <moon_in_france@yahoo.co.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pandanoir/unitaryjs/issues"
  },
  "homepage": "https://github.com/pandanoir/unitaryjs"
}
